{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["react", "react-hooks"],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "off",
      "prettier/prettier": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
}
